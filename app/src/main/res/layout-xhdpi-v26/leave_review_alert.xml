<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="370dp"
    android:layout_height="280dp"
    android:layout_marginStart="19dp"
    android:background="@drawable/rounded_list_obj">


    <Button
        android:id="@+id/ok"
        android:layout_width="100dp"
        android:layout_height="53dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="28dp"
        android:background="@drawable/rounded_corners"
        android:backgroundTint="#2F5685"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/ok"
        android:textColor="#CFBFBF"
        android:textSize="24sp"
        app:layout_constraintEnd_toStartOf="@+id/annulla_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <Button
        android:id="@+id/annulla_button"
        android:layout_width="134dp"
        android:layout_height="53dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/rounded_corners"
        android:backgroundTint="#A52222"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/annulla"
        android:textColor="#CFBFBF"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/attenzione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:autoSizeTextType="uniform"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/attenzione"
        android:textAlignment="center"
        android:textColor="#D31616"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="358dp"
        android:layout_height="141dp"
        android:fontFamily="@font/poppins_bold"
        android:text="@string/tornando_alla_schermata_precedente_perderai_i_dati_relativi_alla_recensione_che_si_sta_inserendo"
        android:textAlignment="viewStart"
        android:textColor="#DCD3D3"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attenzione" />
</androidx.constraintlayout.widget.ConstraintLayout>