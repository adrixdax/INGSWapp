<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="15dp"
        android:height="2dp"
        android:top="10dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#9e9e9e"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="20dp"
        android:height="2dp"
        android:top="7.8dp"
        android:left="9dp">
        <rotate
            android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="#9e9e9e"/>
            </shape>
        </rotate>
    </item>

</layer-list>